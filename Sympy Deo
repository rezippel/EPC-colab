      "source": [
        "def fib(n, a=symbols('a'), b=symbols('b')):\n",
        "  if n < 1:\n",
        "    return symbols('a')\n",
        "  else:\n",
        "    x, a, b = 0, a, b\n",
        "    for i in range(n):\n",
        "      x, a,b = a, b, a+b\n",
        "    return b"
      ],
      "cell_type": "code",
      "execution_count": null,
      "outputs": []
