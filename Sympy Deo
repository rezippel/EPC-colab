        "def evenp(x):\n",
        "  if isinstance(x, int):\n",
        "    return x % 2 == 0\n",
        "  else:\n",
        "    raise ValueError(\"`{}` is not a valid argument for odd_or_even\".format(x))\n",
        "\n",
        "def odd_or_even():\n",
        "  if evenp(raw_input(\"what is a integer?\")):\n",
        "    print (\"true\")\n",
        "  else:\n",
        "    print(\"not even\")\n",
