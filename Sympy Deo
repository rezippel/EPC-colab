        "def eval (x):\n",
        "  if isinstance(x, int):\n",
        "    return x\n",
        "  elif x[0] == 'plus':\n",
        "    return eval(x[1]) + eval(x[2])\n",
        "  elif x[0] == 'times':\n",
        "    return eval(x[1]) * eval(x[2])\n",
        "\n",
