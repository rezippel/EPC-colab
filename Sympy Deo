        "class NSet(object):\n",
        "  def __init__(self):\n",
        "    self._values_ = []\n",
        "\n",
        "  def __repr__ (self):\n",
        "    return 'NSet{}'.format(self._values_)\n",
        "\n",
        "  def add(self, val):\n",
        "    if val not in self._values_:\n",
        "      self._values_.append(val)\n",
        "\n",
        "\n",
        "n = NSet()\n",
        "n.add(1)\n",
        "n.add(1)\n",
        "n.add(1)\n",
        "n.add(1)\n",
        "n"
